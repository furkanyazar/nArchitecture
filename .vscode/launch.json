  "version": "0.2.0",
  "configurations": [
    {
      "name": "Kodlama.io.Devs",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/demoProjects/kodlama.io.Devs/Kodlama.io.Devs.WebAPI/bin/Debug/net6.0/Kodlama.io.Devs.WebAPI.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/demoProjects/kodlama.io.Devs/Kodlama.io.Devs.WebAPI",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": "RentACar",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/demoProjects/rentACar/RentACar.WebAPI/bin/Debug/net6.0/RentACar.WebAPI.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/demoProjects/rentACar/RentACar.WebAPI",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    }
  ]
}
